@model IEnumerable<BillingWeb.tblProductCategory>

@{
    ViewBag.Title = "Index";
}

<div class="detail-box">

    @if (((tblProductCategory)ViewBag.ProductCategory).ProductCategoryID > 0)
    {
        Html.RenderPartial("Edit", (tblProductCategory)ViewBag.ProductCategory);
    }
    else
    {
        Html.RenderPartial("Create", (tblProductCategory)ViewBag.ProductCategory);
    }

</div>
